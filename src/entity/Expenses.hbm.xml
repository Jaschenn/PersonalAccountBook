<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.Expenses" table="Expenses">
        <id name="uid" column="uuid">
            <generator class="uuid2"></generator>
        </id>
        <property name="account" column="t_account"></property>
        <property name="amount" column="t_amount"></property>
        <property name="time" column="t_time"></property>
        <property name="introduction" column="t_introduction"></property>
    </class>
</hibernate-mapping>