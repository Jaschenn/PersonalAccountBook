<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.Income" table="Income">
        <id name="uid" column="uuid">
            <generator class="uuid2"></generator>
        </id>
        <property name="accountName" column="t_accountName"></property>
        <property name="accountUuid" column="t_accountUuid"></property>
        <property name="amount" column="t_amount"></property>
        <property name="time" column="t_time"></property>
        <property name="source" column="t_source"></property>

    </class>
</hibernate-mapping>